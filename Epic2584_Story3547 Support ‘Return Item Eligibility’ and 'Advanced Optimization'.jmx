inner join (
	select max(cast(ExternalReferenceID AS INT )) erid 
	FROM PromotionHeader
	where matrixmemberid=@matrixmemberid) MaxPH
		ON MaxPH.erid = cast(ph.ExternalReferenceID AS INT)
