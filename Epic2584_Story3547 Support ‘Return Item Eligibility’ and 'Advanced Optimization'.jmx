                                                            String NodeValue = eElement.getAttributes().getNamedItem(Attribute).getNodeValue();
                                                            //log.info(NodeValue);
                                                            return NodeValue;
                                                            
                                             }
                              }
                              
               }
               catch(Exception e){

                              log.info(&quot;XMLExtractor&quot; + e);
                              
               }
                              
}

