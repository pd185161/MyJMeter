WHERE        (CRM_Clubcard.MatrixMemberId = @matrixmemberid)
GROUP BY CRM_Clubcard.ClubCardId, CRM_BuyingUnitPromotionHistory.UpdatedDate, CRM_BuyingUnitPromotionHistory.PromotionHeaderId
HAVING        (COUNT(CRM_Clubcard.ClubCardId) &gt; 1) and CRM_Clubcard.ClubCardId  not like &apos;%-%&apos;
