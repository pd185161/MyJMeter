          <stringProp name="queryType">Select Statement</stringProp>
          <stringProp name="query">Select C.ClubId from CRM_RetailerClubAssign as CA
JOIN CRM_Club AS C
ON CA.ClubInternalKey = C.ClubInternalKey
JOIN Retailer_MP AS R
